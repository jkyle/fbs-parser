@ITEM SWORD

START >>
- SWORD.strength <- 10
- SWORD.takeable <- false
- SWORD.name <- "Magic Sword"

TAKE >>
? SWORD.takeable is false
  - ((No you don't. The sword is stuck.))
x
? $INVENTORY has SWORD
  - ((You already have {@ SWORD.name }.
    Do you need another?))
x

LOOK >>
- ((
  {@ SWORD.name } gleams in the sunlight.
  {? SWORD.takeable is false} It seems to be stuck in the stone. {x}
))
